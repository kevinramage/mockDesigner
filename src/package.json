  "description": "Mock Designer help you to generate code to create easily your mock",
  "main": "mockGeneration.js",
  "scripts": {
    "start": "npm run build && node dist/mockGeneration.js",
    "build": "tsc",
    "test": "./integration/venom.linux-amd64 run --output-dir . ./integration/*.yml",
