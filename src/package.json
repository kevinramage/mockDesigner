    "express": "^4.17.1",
    "express-validator": "^6.6.1",
    "mocha": "^8.1.3",
    "morgan": "^1.10.0",
    "querystring": "^0.2.0",
    "redis": "^2.8.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7",
